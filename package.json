{
	"name": "folderize",
	"displayName": "Folderize",
	"description": "Easily convert a Javascript file into a folder. Supports .js .jsx, .ts, .tsx, etc",
	"version": "0.0.1",
	"publisher": "ee92",
	"repository": {
		"type": "git",
		"url": "https://github.com/ee92/vscode-file-to-folder"
	},
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.folderize"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.folderize",
				"title": "Folderize"
			}
		],
		"menus": {
			"editor/context": [
			  {
				 "command": "extension.folderize",
				 "group": "boilerplate@1"
			  }
			],
			"explorer/context": [
				{
				  "command": "extension.folderize",
				  "group": "boilerplate@1"
				}
			 ]
		 }
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
