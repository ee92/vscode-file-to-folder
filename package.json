{
	"name": "create-react-component-folder",
	"displayName": "create-react-component-folder",
	"description": "creates a react component folder with a module.css test.js and index.js files",
	"version": "0.0.1",
	"publisher": "ee92",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.createReactComponentFolder"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createReactComponentFolder",
				"title": "Create React Component Folder"
			}
		],
		"menus": {
			"editor/context": [
			  {
				 "command": "extension.createReactComponentFolder",
				 "group": "boilerplate@1"
			  }
			],
			"explorer/context": [
				{
				  "command": "extension.createReactComponentFolder",
				  "group": "boilerplate@1"
				}
			 ]
		 }
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
